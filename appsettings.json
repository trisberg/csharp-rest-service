{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Management": {
    "Endpoints": {
      "Port": 9090,
      "Actuator": {
        "Exposure": {
          "Include": [ "*" ]
        }
      }
    }
  },
  "Steeltoe": {
    "Kubernetes": {
      "Bindings": {
        "Enable": true
      }
    }
  },
  "Postgres": {
    "Client": {
      "Username": "user",
      "Password": "password",
      "Database": "development"
    }
  }
}
